<?xml version="1.0"?>
<extension key="be.aivl.sepacustom" type="module">
  <file>sepacustom</file>
  <name>SEPA Customisations</name>
  <description>SEPA customisations for Amnesty Internation Vlaanderen</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <releaseDate></releaseDate>
  <version></version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>Code is on https://github.com/systopia/be.aivl.sepacustom</comments>
  <civix>
    <namespace>CRM/Sepacustom</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
